<div class="container">
    <h4>Products in Cart</h4>
    <table class="table">
        <thead>
            <tr>
                <th>Product</th>
                <th>Price</th>
            </tr>
        </thead>
        <tbody>
            <% products.forEach(product => { %>
             <tr>
                 <td><%= product.title %></td>
                 <td><%= product.price %></td>
             </tr>
            <% }) %>
        </tbody>
    </table>
</div>